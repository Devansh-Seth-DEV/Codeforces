#!/bin/bash

CONTEST_TYPE="$1"
CONTEST_NO="$2"
CONTEST_DIR="./contests"
CONTEST_TYPE_DIR="${CONTEST_DIR}/div${CONTEST_TYPE}"
NEW_CONTEST_DIR="${CONTEST_TYPE_DIR}/${CONTEST_NO}"

mkdir -p $NEW_CONTEST_DIR
mkdir ${NEW_CONTEST_DIR}/bin

cp ${CONTEST_DIR}/build $NEW_CONTEST_DIR
cp ${CONTEST_DIR}/mfile $NEW_CONTEST_DIR

cd ${NEW_CONTEST_DIR}


